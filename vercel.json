{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ytmp3",
      "dest": "api/ytmp3.js"
    },
    {
      "src": "/api/remini",
      "dest": "api/remini.js"
    },
    {
      "src": "/api/tiktok",
      "dest": "api/tiktok.js"
    },
    {
      "src": "/api/igdl",
      "dest": "api/igdl.js"
    },
    {
      "src": "/api/update",
      "dest": "api/update.js"
    },
    {
      "src": "/api/ytdl",
      "dest": "api/ytdl.js"
    },
    {
      "src": "/api/check-host",
      "dest": "api/check-host.js"
    },
    {
      "src": "/api/mediafire",
      "dest": "api/mediafire.js"
    },
    {
      "src": "/api/lirik",
      "dest": "api/lirik.js"
    },
    {
      "src": "/api/apk",
      "dest": "api/apk.js"
    },
    {
      "src": "/api/komiku",
      "dest": "api/komiku.js"
    },
    {
      "src": "/api/fluxai",
      "dest": "api/fluxai.js"
    },
    {
      "src": "/api/speechtotext",
      "dest": "api/speechtotext.js"
    }
  ]
}